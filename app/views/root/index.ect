<!-- index.ect -->
<% extend 'layouts/layout.ect' %>
<h3>Recent Images</h3>
<ul class="thumbnails">
    <% for image in @images : %>
        <li>
            <a class="thumbnail" href="/images/<%- image.id %>"><img src="<%- @h.imageUrl(image.image, {width: 200, height: 200, crop: 'fit'}) %>" alt="<%= image.title %>" /></a>
        </li>
    <% end %>
</ul>
